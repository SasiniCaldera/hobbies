<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.sasini.hobby_app">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/notes_icon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/notes_icon"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.sasini.hobby_app.RegistrationActivity"></activity>
        <activity android:name="com.sasini.hobby_app.HomeActivity" />
        <activity android:name="com.sasini.hobby_app.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>